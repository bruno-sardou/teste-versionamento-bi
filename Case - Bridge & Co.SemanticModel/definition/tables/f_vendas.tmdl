	partition f_vendas = m
		mode: import
		source =
				let
				    Fonte = Odbc.Query("dsn=bridge_db", "SELECT #(lf)    V.*,#(lf)    CASE#(lf)        WHEN data_venda < data_admissao THEN ""Não""#(lf)        ELSE ""Sim""#(lf)    END AS venda_validada#(lf)FROM#(lf)    vendas V#(lf)LEFT JOIN#(lf)    rh R#(lf)    ON V.idvendedor = r.idvendedor #(lf)")
				in
				    Fonte

